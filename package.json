{
  "name": "got-commit",
  "displayName": "Got Commit",
  "description": "",
  "version": "1.0.2",
  "publisher": "heappynd",
  "icon": "icons/git.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/heappynd/got-commit.git"
  },
  "license": "MIT",
  "author": {
    "name": "heappynd",
    "email": "heappynd@163.com"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:got-commit.commit"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "title": "Got Commit configuration",
      "properties": {
        "gotcommit.jiraPrefix": {
          "type": "string",
          "default": "",
          "description": "If this is set it will be will be displayed as the default JIRA ticket prefix"
        }
      }
    },
    "commands": [
      {
        "command": "got-commit.commit",
        "title": "Go commit",
        "icon": "icons/git.svg"
      }
    ],
    "menus": {
      "scm/title": [
        {
          "when": "scmProvider == git",
          "command": "got-commit.commit",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@vscode/test-electron": "^2.2.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "ts-loader": "^9.4.1",
    "typescript": "^4.9.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.0"
  },
  "dependencies": {
    "execa": "^6.1.0"
  }
}
